{
  "kind": "collectionType",
  "collectionName": "gallery_types",
  "info": {
    "name": "Gallery Type",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "maxLength": 10,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "Image": {
      "type": "component",
      "repeatable": true,
      "component": "media.image",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "GalleryPageImages": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "media.gallery-image"
    }
  }
}
